.ick-onlinechat {
  position fixed
  right -100px
  top 50%
  width 100px
  text-align center
  transform translateY(-50%)
  .ick-onlinechat__img{
    position absolute
    left -40px
    top 50%
    transform translateY(-50%)
    cursor pointer
  }
  .ick-onlinechat__content {
    display flex
    flex-direction column
    .bigsize {
      font-size 14px
    }
    .phone {
      font-size: 12px
      color: #f60
      height: 17px
      margin-bottom 6px
      line-height: 28px
      vertical-align top
      display inline-block
    }
    .wangwang-title {
      margin: 0 0 0 5px
      color: #333
    }
    .qq-title {
    }
    .chat-img {
      width 100%
      height  100%
    }
  }
}